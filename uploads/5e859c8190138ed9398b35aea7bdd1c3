
  #include <stdio.h>

int main()
{
  int i,j,n;
  int temp=0;
    int arr[100];
  printf("Enter the size of an array : ");
  scanf("%d",&n);
printf("Enter the array elements :) ");
  for(i=0;i<n;i++){
      for(j=i+1;j<n-1;j++){
          if(arr[i]>arr[j]){
              temp=arr[i];
              arr[j]=arr[i];
              arr[i]=temp;
          }
      }
      
  }
  printf("%d",arr[j]);

    return 0;
}
  
